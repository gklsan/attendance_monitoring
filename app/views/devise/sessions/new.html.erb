<div id ="form"> 
<h2> Log in </h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email, :class => "labelbox" %>
    <%= f.email_field :email, autofocus: true, height: 30, width:60, :class => "textbox" %>
  </div>

  <div class="field">
    <%= f.label :password, :class => "labelbox" %>
    <%= f.password_field :password, autocomplete: "off", :class => "textbox" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <div class ="labelbox" >
      <%= f.check_box :remember_me %><b> Remember_me</b> 
    </div>
    </div>
  <% end -%>

  
    <%= f.submit "Sign in", :class => 'all-button' %>
   <% end %>  



   <%= render "devise/shared/links" %> 
